<script src="https://xqq.im/mpegts.js/dist/mpegts.js"></script>
<video id="videoElement" class="centeredVideo" autoplay="autoplay" controls="controls" width="100%" height="450"></video>
<script>
    if (mpegts.getFeatureList().mseLivePlayback) {
        var videoElement = document.getElementById('videoElement');
        var player = mpegts.createPlayer({
            type: 'm2ts',  // could also be mpegts, m2ts, flv
            isLive: true,
            url: 'https://nonton.site:8443/tweety875368/mskls35790ysjk/78'
        });
        player.attachMediaElement(videoElement);
        player.load();
        player.play();
    }
</script>
